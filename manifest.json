{
  "manifest_version": 2,
  "name": "ArXiv Redirector",
  "version": "1.0",
  "description": "Redirects ArXiv URLs to AlphaXiv",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "webNavigation",
    "storage"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
